{
  "MongoDbSettings": {
    "ConnectionString": "mongodb://inventorydb:27017",
    "DatabaseName": "InventoryDb",
    "CollectionName": "InventoryEntries"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc": "Debug"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80",
        "Protocols": "Http1"
      },
      "Grpc": {
        "Url": "http://+:5005",
        "Protocols": "Http2"
      }
    }
  }
}
